/* MEANS2.SAS : MEANS PROCEDURE */
OPTIONS PAGESIZE=50 NODATE;
DATA A;
  INPUT RATING EXCESS PLACE $ DAY @@;
  DATALINES;
04 54 S 1   07 70 N 1   10 69 N 2   04 52 S 1
07 70 S 2   08 74 N 1   04 60 S 1   07 62 S 2
07 80 N 1   06 61 S 2   06 77 N 2   08 75 N 2
;
RUN;
PROC SORT DATA=A;
  BY PLACE DAY;
RUN;
PROC MEANS DATA=A MAXDEC=3;
  TITLE2 'OUTPUT FROM MEANS PROCEDURES';
  BY PLACE DAY;
  VAR RATING EXCESS;
  OUTPUT OUT=NEW MEAN=RMEAN EMEAN STDERR=RSE ESE;
  TITLE3 'STATISTICS BY PLACE AND DAY';
RUN;
PROC PRINT DATA=NEW ROUND;
  TITLE3 'NEW DATA SET';
RUN;   QUIT;
