/* ANOVA4.SAS : TWO-WAY LAYOUT - MULTIPLE COMPARISON */
OPTIONS PS=60 PAGENO=1;
DATA FERTIL;
  INPUT BLOCK $ TRTMENT $ YIELD @@;
  DATALINES;
1 A 32.6    1 B 36.4    1 C 29.5
2 A 42.7    2 B 47.1    2 C 32.9
3 A 35.3    3 B 40.1    3 C 33.6
;
RUN;
PROC ANOVA;
  CLASS BLOCK TRTMENT;
  MODEL YIELD = BLOCK TRTMENT;
  MEANS TRTMENT / LSD DUNCAN TUKEY ALPHA=.1;
RUN; QUIT;
